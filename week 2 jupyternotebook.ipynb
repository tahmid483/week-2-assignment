    "nums = [1, 2, 2, 3, 4, 4, 5]\n",
    "squared = {x**2 for x in nums}\n",
    "print(\"Squared Set:\", squared)\n",
    "\n",
    "even_squares = {x**2 for x in nums if x % 2 == 0}\n",
    "print(\"Even Squares Set:\", even_squares)\n",
    "\n",
    "sorted_squares = sorted({x**2 for x in nums})\n",
    "print(\"Sorted Squares:\", sorted_squares)"
